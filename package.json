{
  "name": "semantic-release-helm-v3",
  "description": "semantic-release plugin to publish a Helm release",
  "version": "0.1.0",
  "author": "Thanh Duong <thanh@trinity-technology.com>",
  "type": "module",
  "ava": {
    "files": [
      "test/**/*.test.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/trinitytechnology/semantic-release-helm-v3/issues"
  },
  "contributors": [],
  "dependencies": {
    "@semantic-release/error": "^4.0.0",
    "aggregate-error": "^5.0.0",
    "debug": "^4.0.0",
    "dir-glob": "^3.0.0",
    "escape-string-regexp": "^5.0.0",
    "formdata-node": "^6.0.3",
    "fs-extra": "^11.0.0",
    "globby": "^14.0.0",
    "got": "^14.0.0",
    "hpagent": "^1.0.0",
    "lodash-es": "^4.17.21",
    "parse-url": "^9.0.0",
    "execa": "^5.0.0",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "prettier": "3.3.3",
    "semantic-release": "24.1.2"
  },
  "engines": {
    "node": ">=20.8.1"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "homepage": "https://github.com/trinitytechnology/semantic-release-helm-v3#readme",
  "keywords": [
    "git",
    "helm",
    "publish",
    "release",
    "semantic-release",
    "version"
  ],
  "license": "MIT",
  "main": "index.js",
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "peerDependencies": {
    "semantic-release": ">=20.1.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trinitytechnology/semantic-release-helm-v3.git"
  },
  "scripts": {
    "lint": "prettier --check \"*.{js,json,md}\" \".github/**/*.{md,yml}\" \"{bin,lib,test}/*.js\"",
    "lint:fix": "prettier --write \"*.{js,json,md}\" \".github/**/*.{md,yml}\" \"{bin,lib,test}/*.js\"",
    "semantic-release": "semantic-release"
  },
  "renovate": {
    "extends": [
      "github>semantic-release/.github:renovate-config"
    ]
  }
}
